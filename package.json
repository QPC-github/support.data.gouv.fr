{
  "name": "support.data.gouv.fr",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "run-p build:*",
    "build:client": "webpack",
    "build:prod": "tsc --project tsconfig.json",
    "clean": "rimraf public/js",
    "lint": "eslint src --ext .js,.ts",
    "start": "npm run start:prod",
    "start:dev": "run-p clean start:dev:*",
    "start:dev:client": "webpack --watch --mode=development --devtool=eval-source-map",
    "start:dev:server": "ts-node-dev --rs --watch 'src/**/*.ts,views/**/*.nj,src/**/*.yaml' src/infrastructure",
    "start:prod": "node dist/infrastructure",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etalab/support.data.gouv.fr.git"
  },
  "license": "AGPLv3",
  "bugs": {
    "url": "https://github.com/etalab/support.data.gouv.fr/issues"
  },
  "homepage": "https://github.com/etalab/support.data.gouv.fr#readme",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/nunjucks": "^3.1.4",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "@hotwired/turbo": "^7.0.0-beta.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "nunjucks": "^3.2.3",
    "yaml": "^1.10.2"
  }
}
